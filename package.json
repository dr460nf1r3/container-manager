{
  "name": "container-manager",
  "version": "2.1.0",
  "private": true,
  "description": "Simple NestJs backend for creating and managing docker containers, which run a set of Docker containers via DinD",
  "homepage": "https://github.com/dr460nf1r3/container-manager#readme",
  "bugs": {
    "url": "https://github.com/dr460nf1r3/container-manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr460nf1r3/container-manager.git"
  },
  "license": "AGPL-3.0",
  "author": "dr460nf1r3",
  "scripts": {
    "build": "nest build --builder=webpack",
    "commit": "cz",
    "documentation": "pnpm swagger && openapi-to-md docs/swagger.json >docs/api.md && compodoc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,assets,test}/**/*.ts\" --fix",
    "release": "git tag $(git cliff --bumped-version) -m $(git cliff --bumped-version)",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "swagger": "SWAGGER_JSON=true nest start",
    "test": "jest",
    "test-e2e": "jest --config ./test/jest-e2e.json",
    "test-rl": "bash test/e2e.sh"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.2",
    "@fastify/reply-from": "^12.3.1",
    "@fastify/static": "^8.2.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-spawn": "^7.0.6",
    "dockerode": "^4.0.9",
    "fastify": "5.6.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.30",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@types/dockerode": "^3.3.44",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "git-cliff": "^2.10.1",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "openapi-to-md": "^1.0.26",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@compodoc/compodoc",
      "@nestjs/core",
      "protobufjs",
      "unrs-resolver"
    ]
  }
}
