{
  "name": "container-manager",
  "version": "1.2.0",
  "description": "Simple NestJs backend for creating and managing docker containers, which run a set of Docker containers via DinD",
  "author": "dr460nf1r3",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "nest build",
    "commit": "cz",
    "documentation": "pnpm swagger && openapi-to-md docs/swagger.json >docs/api.md && compodoc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "release": "git tag $(git cliff --bumped-version) -m $(git cliff --bumped-version)",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "swagger": "SWAGGER_JSON=true nest start",
    "lint": "eslint \"{src,assets,test}/**/*.ts\" --fix",
    "test": "jest",
    "test-e2e": "jest --config ./test/jest-e2e.json",
    "test-rl": "bash test/e2e.sh"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/reply-from": "^10.0.0",
    "@fastify/static": "^7.0.4",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-fastify": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/throttler": "^6.3.0",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-spawn": "^7.0.6",
    "dockerode": "^4.0.2",
    "fastify": "4.28.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/dockerode": "^3.3.32",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "git-cliff": "^2.7.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "openapi-to-md": "^1.0.25",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.0.0-rc.0+sha512.bce495587126f9a37d64150bf80dcf9bd96f0d64cfd450c9b1a7e5bf2cfc81aef3d50178f259855351c872905a240861a55031652c81e8a11665bfa65f6a1dc2",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
