{
  "name": "container-manager",
  "version": "2.1.0",
  "private": true,
  "description": "Simple NestJs backend for creating and managing docker containers, which run a set of Docker containers via DinD",
  "homepage": "https://github.com/dr460nf1r3/container-manager#readme",
  "bugs": {
    "url": "https://github.com/dr460nf1r3/container-manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr460nf1r3/container-manager.git"
  },
  "license": "AGPL-3.0",
  "author": "dr460nf1r3",
  "scripts": {
    "build": "nest build --builder=webpack",
    "commit": "cz",
    "documentation": "pnpm swagger && openapi-to-md docs/swagger.json >docs/api.md && compodoc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,assets,test}/**/*.ts\" --fix",
    "release": "git tag $(git cliff --bumped-version) -m $(git cliff --bumped-version)",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "swagger": "SWAGGER_JSON=true nest start",
    "test": "jest",
    "test-e2e": "jest --config ./test/jest-e2e.json",
    "test-rl": "bash test/e2e.sh"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.1",
    "@fastify/reply-from": "^12.0.2",
    "@fastify/static": "^8.1.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/throttler": "^6.4.0",
    "axios": "^1.8.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-spawn": "^7.0.6",
    "dockerode": "^4.0.4",
    "fastify": "5.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@types/dockerode": "^3.3.35",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "git-cliff": "^2.8.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "openapi-to-md": "^1.0.25",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "protobufjs",
      "@compodoc/compodoc",
      "@nestjs/core"
    ]
  }
}
